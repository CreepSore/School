<Application x:Class="AvgCalc.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AvgCalc"
             xmlns:converter="clr-namespace:BSWPFLib.Converter;assembly=BSWPFLib"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converter:InvBoolConverter x:Key="InvBoolConverter"/>
        <converter:VisibilityConverter x:Key="VisibilityConverter"/>
        <converter:InvVisibilityConverter x:Key="InvVisibilityConverter"/>
    </Application.Resources>
</Application>
